/* components/Snowflakes/index.wxss */

div.snow-front-wrapper{
  position: absolute;
  top: 0;
  height: 100vh;
  width: 100vw;
  display: block;
  animation: sfw 6s cubic-bezier(0.445, 0.05, 0.55, 0.95) infinite alternate;
}

div.snow-front-wrapper .snowflake{
  display: block;
  top: -50px;
  width: 20px;
  height: 20px;
  background-image: url(data:image/png;base64,);
  background-size: 96%;
  position: absolute;
  animation: sf 5s cubic-bezier(0.445, 0.05, 0.55, 0.6) infinite;
}


div.snow-front-wrapper .snowflake:nth-of-type(1) {
  animation-duration: 5s;
  animation-delay: 2.5s;
  left: -25vw;
}

div.snow-front-wrapper .snowflake:nth-of-type(2) {
  animation-duration: 7s;
  animation-delay: 1s;
  left: 5vw;
}

div.snow-front-wrapper .snowflake:nth-of-type(3) {
  animation-duration: 4s;
  left: 35vw;
}
div.snow-front-wrapper .snowflake:nth-of-type(4) {
  animation-duration: 6s;
  animation-delay: 1.5s;
  left: 65vw;
}
div.snow-front-wrapper .snowflake:nth-of-type(5) {
  animation-duration: 3s;
  animation-delay: 0.5s;
  left: 95vw;
}
div.snow-front-wrapper .snowflake:nth-of-type(6) {
  animation-duration: 5s;
  animation-delay: 2s;
  left: 125vw;
}

@keyframes sf {
  to {
    top: 110vh;
  }
}

@keyframes sfw {
  0% {
    left: -25vw;
  }
  100%{
    left: 25vw;
  }
}

